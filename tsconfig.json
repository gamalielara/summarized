{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "noImplicitAny": true,
    "paths": {
      "<screens>/*": [
        "./src/screens/*"
      ],
      "<components>/*": [
        "./src/components/*"
      ],
      "<utils>/*": [
        "./src/utils/*"
      ],
      "<src>/*": [
        "./src/*"
      ],
      "<assets>/*": ["./src/assets/*"]
    }
  }
}
